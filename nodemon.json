{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    "test/fixtures/",
    "test/samples/"
  ],
  "verbose": true,
  "execMap": {
    "js": "node --harmony"
  },
  "watch": [
    "src/*"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "ts js json"
}
